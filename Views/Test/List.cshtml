@model System.Collections.Generic.IList<HelloWorldProject2.Model.Student>
<h3>List View</h3>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Score</th>
    </tr>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.Id</td>
            <td>@student.Name</td>
            <td>@student.Score</td>
        </tr>
    }
</table>